<p id="notice"><%= notice %></p>

<h1>Details</h1>

<table>
  <thead>
    <tr>
      <th>Average</th>
      <th>Maximum</th>
      <th>Minimum</th>
      <th>Total Query</th>
      <th>Total Fail</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @details.each do |detail| %>
      <tr>
        <td><%= detail.average %></td>
        <td><%= detail.maximum %></td>
        <td><%= detail.minimum %></td>
        <td><%= detail.total_query %></td>
        <td><%= detail.total_fail %></td>
        <td><%= detail.status %></td>
        <td><%= link_to 'Show', detail %></td>
        <td><%= link_to 'Edit', edit_detail_path(detail) %></td>
        <td><%= link_to 'Destroy', detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Detail', new_detail_path %>
